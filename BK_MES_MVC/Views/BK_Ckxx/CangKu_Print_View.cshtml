@{
    ViewBag.Title = "Report";
    Layout = "~/Views/Shared/_Layout_2018.cshtml";
}

@section scripts {
    @Scripts.Render("~/bundles/table-alert")
    @Styles.Render("~/Content/table-alert")

    <!--验证-->
    @Scripts.Render("~/bundles/bootstrapValidator")
    @Styles.Render("~/Content/bootstrapValidator")

    <link href="~/Content/bootstrap-select.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap-select.js" type="text/javascript"></script>
    <script src="~/Scripts/bootstrap-select-zh_CN.js" type="text/javascript"></script>

    <script src="~/Scripts/Bk_Ckxx/CangKu_Print_View.js"></script>
}

<!--表格-->
    <div class="panel-body" style="padding:0px;">

        <div class="container">
            <div class="row">
                        <form class="form-inline">
                            <div class="form-group" style="width:200px">
                                @Html.DropDownList("ckmc", (List<SelectListItem>)ViewBag.form_ckmc, "请选择仓库", new { @class = "selectpicker form-control", data_style = "btn-white", data_live_search = "true", data_size = "5", styles = "width:200px" })
                            </div>
                            <div class="form-group" style="width:200px">
                                @Html.DropDownList("pmc", (List<SelectListItem>)ViewBag.form_pmc, "请选择仓库", new { @class = "selectpicker form-control", data_style = "btn-white", data_live_search = "true", data_size = "5", styles = "width:200px" })
                            </div>
                            <div class="form-group" style="width:150px">
                                <label>层</label>
                                <input type="text" class="form-control" id="ceng1" name="ceng1" style="width:50px" oninput="value=value.replace(/[^\d]/g,'')">
                                <input type="text" class="form-control" id="ceng2" style="width:50px" oninput="value=value.replace(/[^\d]/g,'')">
                            </div>
                            <div class="form-group" style="width:150px">
                                <label>列</label>
                                <input type="text" class="form-control" id="wei1" style="width:50px" oninput="value=value.replace(/[^\d]/g,'')">
                                <input type="text" class="form-control" id="wei2" style="width:50px" oninput="value=value.replace(/[^\d]/g,'')">
                            </div>

                            <button type="button" class="btn btn-primary queryButton" id="btn_print">查询</button>
                        </form>
                </div>
        </div>
    </div>
      