@model  BK_MES_MVC.Models.tmpa
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Temp</title>
    <script src="~/Scripts/jquery.min.js" type="text/javascript"></script>


    <script>
        $(function () {// 初始化内容
            $("#btn_1").click(function () {
                $.ajax(
                    {
                        url: "/bk_ckxx/temp?id=11"
                        , async: false
                        , success: function (data) {
                            //alert(data.sb);
                            $("#abc").html(data.sb);
                        }
                    }
                );
            });
        });

    </script>
</head>
<body>
    <div>
        <a href="/bk_ckxx/temp?id=1">temp</a>
        <button id="btn_1" name="btn_1" value="bbbbb" type="button">BBBBBBB</button>
        <div id="abc" name="abc">
            1
            2
        </div>

    </div>
</body>
</html>
